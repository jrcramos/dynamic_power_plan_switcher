{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./dist-electron",
    "target": "ES2020",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": false,
    "allowJs": true,
    "types": ["node"]
  },
  "include": [
    "electron/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "dist-electron"
  ]
}
